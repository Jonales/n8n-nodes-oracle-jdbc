{
  "name": "n8n-nodes-oracle-jdbc",
  "version": "1.2.0",
  "description": "Oracle Database nodes using pure JDBC driver (zero Oracle Client dependencies). Enterprise-grade Oracle connectivity for n8n with pooling, transactions, stored procedures, RAC/ADG and health-monitoring.",
  "keywords": [
    "n8n-community-node-package",
    "oracle",
    "jdbc",
    "database",
    "java",
    "sql",
    "enterprise",
    "connection-pool",
    "transactions",
    "stored-procedures",
    "oracle-19c",
    "oracle-21c",
    "oracle-23c",
    "oracle-23ai",
    "autonomous-database",
    "rac",
    "adg",
    "zero-external-config",
    "cloud-compatible"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jonales/n8n-nodes-oracle-jdbc",
  "author": {
    "name": "JÃ´natas Meireles Sousa Vieira",
    "email": "jonatas.mei@outlook.com",
    "url": "https://github.com/jonales"
  },
  "contributors": [
    {
      "name": "Community Contributors",
      "url": "https://github.com/jonales/n8n-nodes-oracle-jdbc/graphs/contributors"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonales/n8n-nodes-oracle-jdbc.git",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/jonales/n8n-nodes-oracle-jdbc/issues",
    "email": "jonatas.mei@outlook.com"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "lib/**/*.jar",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md",
    "docs/**/*",
    "icons/**/*"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=10.0.0",
    "java": ">=11.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "packageManager": "npm@10.9.0",
  
  "scripts": {
    "clean": "npx rimraf dist .cache tsconfig.tsbuildinfo *.log && npm run cache:clean",
    "clean:deep": "npm run clean && npx rimraf node_modules lib && npm install",
    "clean:cache": "npx rimraf .cache .eslintcache",
    "clean:dist": "npx rimraf dist",
    "clean:logs": "npx rimraf *.log scripts/**/*.log",
    
    "setup": "npm run setup:env && npm run setup:java && npm run download:jdbc && npm run verify:setup",
    "setup:env": "node scripts/setup-environment.js",
    "setup:java": "node scripts/setup-java.js",
    "setup:java:verbose": "node scripts/setup-java.js --verbose",
    "setup:ci": "npm run setup:java && npm run download:jdbc:ci && npm run verify:setup:ci",
    
    "download:jdbc": "node scripts/download-jdbc.js",
    "download:jdbc:verbose": "node scripts/download-jdbc.js --verbose",
    "download:jdbc:force": "node scripts/download-jdbc.js --force",
    "download:jdbc:ci": "node scripts/download-jdbc.js --ci",
    "download:jdbc:check": "node scripts/check-jdbc-drivers.js",
    
    "verify:setup": "node scripts/verify-setup.js",
    "verify:setup:ci": "node scripts/verify-setup.js --ci",
    "verify:drivers": "node scripts/verify-jdbc-drivers.js",
    "test:connection": "node scripts/test-oracle-connection.js",
    "test:connection:all": "node scripts/test-all-connections.js",
    "healthcheck": "node scripts/health-check.js",
    
    "prebuild": "npm run clean && npm run verify:drivers",
    "build": "npm run build:ts && npm run build:assets && npm run build:verify",
    "build:fast": "tsc -p tsconfig.json",
    "build:ts": "tsc -p tsconfig.json --incremental",
    "build:ts:watch": "tsc -p tsconfig.json --watch --incremental",
    "build:assets": "gulp build:assets",
    "build:complete": "npm run build:ts && npm run build:assets && npm run build:docs",
    "build:verify": "node scripts/verify-build.js",
    "build:docs": "node scripts/generate-docs.js",
    "build:icons": "gulp build:icons",
    "build:ci": "npm run build && npm run validate:ci",
    
    "dev": "npm run build:ts:watch & npm run dev:assets",
    "dev:assets": "gulp watch",
    "dev:test": "npm run test:watch",
    "dev:lint": "npm run lint -- --watch",
    
    "format": "prettier . --write --ignore-path .gitignore --cache --cache-location .cache/prettier/",
    "format:check": "prettier . --check --ignore-path .gitignore --cache --cache-location .cache/prettier/",
    "format:staged": "lint-staged",
    "lint": "eslint . --ext .ts,.js --cache --cache-location .cache/eslint/ --cache-strategy metadata",
    "lint:fix": "eslint . --ext .ts,.js --fix --cache --cache-location .cache/eslint/ --cache-strategy metadata",
    "lint:ci": "eslint . --ext .ts,.js --cache --cache-location .cache/eslint/ --cache-strategy content --max-warnings 0",
    "lint:staged": "eslint --fix --cache --cache-location .cache/eslint/ --cache-strategy metadata",
    "lint:perf": "TIMING=1 eslint . --ext .ts,.js",
    "lint:clean": "npx rimraf .cache/eslint .eslintcache",
    
    "type-check": "tsc --noEmit -p tsconfig.json",
    "type-check:watch": "tsc --noEmit -p tsconfig.json --watch",
    "test": "jest --passWithNoTests --cache --cacheDirectory .cache/jest/",
    "test:watch": "jest --watch --passWithNoTests --cache",
    "test:coverage": "jest --coverage --passWithNoTests --cache",
    "test:ci": "jest --ci --coverage --passWithNoTests --cache --cacheDirectory .cache/jest/",
    "test:integration": "jest --testMatch='**/*.integration.test.ts' --runInBand",
    "test:oracle": "node scripts/test-oracle-functionality.js",
    
    "benchmark": "node scripts/benchmark.js",
    "benchmark:pool": "node scripts/benchmark-pool.js",
    "benchmark:query": "node scripts/benchmark-query.js",
    "benchmark:all": "npm run benchmark && npm run benchmark:pool && npm run benchmark:query",
    "perf:build": "TIMING=1 npm run build",
    "perf:test": "npm run test -- --verbose",
    "profile": "node --inspect scripts/profile.js",
    
    "cache:clean": "npx rimraf .cache",
    "cache:stats": "node scripts/cache-stats.js",
    "cache:eslint:clean": "npx rimraf .cache/eslint .eslintcache",
    "cache:jest:clean": "npx rimraf .cache/jest",
    "cache:tsc:clean": "npx rimraf .cache/tsconfig.tsbuildinfo tsconfig.tsbuildinfo",
    "cache:prettier:clean": "npx rimraf .cache/prettier",
    "cache:optimize": "node scripts/optimize-cache.js",
    
    "validate": "npm run type-check && npm run lint:ci && npm run test:ci",
    "validate:ci": "npm run validate && npm run build:verify",
    "prepublishOnly": "npm run validate:ci && npm run build:complete",
    "postpublish": "npm run clean",
    "prepare": "husky install",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "release:beta": "semantic-release --branches beta",
    
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "audit:fix": "npm audit fix",
    "audit:check": "npm audit --audit-level moderate",
    "maintenance": "npm run deps:check && npm run audit:check && npm run cache:optimize"
  },
  
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  
  "n8n": {
    "n8nNodesApiVersion": 1,
    "displayName": "Oracle Database Advanced (JDBC)",
    "credentials": [
      "dist/credentials/OracleJdbc.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Oracle/OracleJdbcDatabase.node.js",
      "dist/nodes/Oracle/OracleJdbcAdvanced.node.js",
      "dist/nodes/Oracle/OracleJdbcPool.node.js",
      "dist/nodes/Oracle/OracleJdbcTransaction.node.js"
    ]
  },
  
  "dependencies": {
    "java": "^0.16.2",
    "uuid": "^10.0.0",
    "sql-formatter": "^15.4.3"
  },
  
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.3.0",
    "prettier": "^3.3.3",
    
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "typescript": "^5.7.2",
    "tsconfig-paths": "^4.2.0",
    
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    
    "gulp": "^5.0.0",
    "del": "^7.1.0",
    "rimraf": "^6.0.1",
    
    "semantic-release": "^22.0.12",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^11.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    
    "n8n-workflow": "^1.82.0"
  },
  
  "peerDependencies": {
    "n8n-workflow": ">=1.60.0"
  },
  
  "peerDependenciesMeta": {
    "n8n-workflow": {
      "optional": false
    }
  },
  
  "optionalDependencies": {
    "download": "^8.0.0",
    "extract-zip": "^2.0.1",
    "axios": "^1.7.8"
  },
  
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write --cache --cache-location .cache/prettier/"
    ],
    "*.{ts,js}": [
      "eslint --fix --cache --cache-location .cache/eslint/ --cache-strategy metadata"
    ]
  },
  
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "cacheDirectory": ".cache/jest",
    "clearMocks": true,
    "collectCoverageFrom": [
      "nodes/**/*.ts",
      "credentials/**/*.ts",
      "core/**/*.ts",
      "types/**/*.ts",
      "utils/**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!dist/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "testMatch": [
      "**/__tests__/**/*.test.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/lib/"
    ],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/nodes/$1",
      "^@core/(.*)$": "<rootDir>/core/$1",
      "^@types/(.*)$": "<rootDir>/types/$1",
      "^@utils/(.*)$": "<rootDir>/utils/$1"
    },
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "testTimeout": 30000
  },
  
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true,
          "tarballDir": "dist"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/*.tgz",
              "label": "Distribution package"
            }
          ]
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": ["CHANGELOG.md", "package.json"],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "test": "__tests__"
  },
  
  "config": {
    "java": {
      "version": "11",
      "required": true,
      "maxMemory": "2g"
    },
    "oracle": {
      "jdbc": {
        "version": "23.6.0",
        "drivers": [
          "ojdbc11.jar",
          "ucp.jar",
          "orai18n.jar",
          "ons.jar"
        ],
        "download": {
          "baseUrl": "https://download.oracle.com/otn-pub/otn_software/jdbc/236",
          "requiresLicense": true
        }
      },
      "features": {
        "connectionPooling": true,
        "transactions": true,
        "storedProcedures": true,
        "racSupport": true,
        "cloudSupport": true,
        "healthMonitoring": true
      }
    },
    "cache": {
      "eslint": ".cache/eslint",
      "jest": ".cache/jest",
      "typescript": ".cache",
      "prettier": ".cache/prettier"
    },
    "build": {
      "parallel": true,
      "incremental": true,
      "sourceMap": true,
      "declarations": true
    }
  },
  
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jonales"
  },
  
  "badges": [
    {
      "url": "https://img.shields.io/npm/v/n8n-nodes-oracle-jdbc",
      "href": "https://www.npmjs.com/package/n8n-nodes-oracle-jdbc",
      "description": "npm version"
    },
    {
      "url": "https://img.shields.io/github/license/jonales/n8n-nodes-oracle-jdbc",
      "href": "https://github.com/jonales/n8n-nodes-oracle-jdbc/blob/main/LICENSE.md",
      "description": "license"
    }
  ]
}
